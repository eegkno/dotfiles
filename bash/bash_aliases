# General aliases

alias ll='ls -alFh'
alias la='ls -A'
alias l='ls -CF'
alias tmux="TERM=screen-256color-bce tmux"
alias dev='$HOME/.dotfiles/tmux/tmux_envs.sh dev 1'
alias dev1='$HOME/.dotfiles/tmux/tmux_envs.sh dev1 1'
alias dev2='$HOME/.dotfiles/tmux/tmux_envs.sh dev2 1'
alias runpy='$HOME/.dotfiles/tmux/tmux_envs.sh runpy'
alias ..='cd ../../'
alias ...='cd ../../../'
